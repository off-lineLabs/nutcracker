    <string name="tutorial_welcome_description">Vamos fazer um tour rápido do seu app de rastreamento de calorias. Vou mostrar as principais funcionalidades para você começar.</string>
    
    <string name="tutorial_calorie_ring_title">Rastreamento Diário de Calorias</string>
