    <string name="tutorial_add_meal_title">Adicionar Refeições</string>
    <string name="tutorial_add_meal_description">Toque aqui para registrar suas refeições. Você pode adicionar refeições personalizadas, escanear códigos de barras ou pesquisar alimentos.</string>
    
