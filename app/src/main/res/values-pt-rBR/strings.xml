    <string name="tutorial_calendar_title">Alterar Data</string>
    <string name="tutorial_calendar_description">Visualize ou edite entradas de diferentes dias. Perfeito para registrar refeições que você esqueceu de rastrear.</string>
    
    <string name="tutorial_analytics_title">Ver Progresso</string>
